@model IEnumerable<CurricullumVitae.Models.Document>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<div>
    <h4>My Curriculum Vitae</h4>
    <hr />

    @foreach (var item in Model)
        {
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(item => item.Description)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(item => item)
        </dd>
    </dl>
        }
</div>
<div>
</div>
